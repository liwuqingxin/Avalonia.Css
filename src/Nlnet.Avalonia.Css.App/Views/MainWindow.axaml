<Window x:Class="Nlnet.Avalonia.Css.App.Views.MainWindow"
        xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:pages="clr-namespace:Nlnet.Avalonia.Css.App.Views.Pages"
        xmlns:views="clr-namespace:Nlnet.Avalonia.Css.App.Views"
        Title="Avalonia fluent theme powered by Nlnet.Avalonia.Css library"
        Width="1000"
        Height="700"
        d:DesignHeight="450"
        d:DesignWidth="800"
        Icon="/Assets/avalonia-logo.ico"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d">

    <Grid RowDefinitions="Auto,*">

        <TabControl x:Name="TabControl"
                    Grid.Row="1"
                    TabStripPlacement="Left" />

    </Grid>
</Window>
