<Application x:Class="Nlnet.Avalonia.Css.App.App"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:fluent="https://www.nlnet.com/avalonia">

    <Application.Styles>
        <FluentTheme />
        <fluent:CssFluentTheme />
    </Application.Styles>

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="avares://Nlnet.Avalonia.SampleAssistant/Assets/Themes/Themes.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

    <Application.Styles>
        <StyleInclude Source="avares://Nlnet.Avalonia.SampleAssistant/Assets/Styles/Styles.axaml" />
    </Application.Styles>

</Application>
